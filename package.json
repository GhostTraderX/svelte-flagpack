{
  "name": "svelte-flagpack",
  "version": "2.0.0",
  "description": "Gorgeous flag components for your Svelte project.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "svelte": "dist/Flag.svelte",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/Flag.svelte",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./flags/*": "./dist/flags/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run generate-flags && vite build",
    "dev": "vite build --watch",
    "generate-flags": "tsx ./src/generate-flags.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GhostTraderX/svelte-flagpack.git"
  },
  "type": "module",
  "author": "GhostTraderX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GhostTraderX/svelte-flagpack/issues"
  },
  "homepage": "https://github.com/GhostTraderX/svelte-flagpack#readme",
  "keywords": [
    "svelte",
    "svelte5",
    "sveltekit",
    "svelte-component",
    "languages",
    "flag",
    "iso",
    "flag set",
    "countries",
    "flag-pack",
    "typescript"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.2",
    "@types/node": "^22.10.5",
    "flagpack-core": "^2.0.0",
    "svelte": "^5.18.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
